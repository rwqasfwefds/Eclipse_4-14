package OverRiding_Ex2;

public class ShapeTest {
	
	public static void main(String[] args) {
		Shape shape = new Shape(); // 모양
		Line line = new Line(); // 선
		Rect rect = new Rect(); // 사각형
		Circle circle = new Circle(); // 원
		
		shape.draw();
		line.draw();
		rect.draw();
		circle.draw();
	}
}
