package ex;

public class GoodClacTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		GoodClac goodclac = new GoodClac();
		
		int [] avg = new int[] {2, 3, 4}
		
		System.out.println(goodclac.add(2, 3));
		System.out.println(goodclac.subtract(2, 3));
		System.out.println(goodclac.average(avg));
	}

}
